{
    "name": "ivankff/yii2-formatter",
    "description": "phone formatter",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "phone formatter"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "email": "ivan@sabint.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/ivankff/yii2-formatter/issues?state=open",
        "source": "https://github.com/ivankff/yii2-formatter"
    },
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "ivankff\\yii2-formatter\\": "src/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}